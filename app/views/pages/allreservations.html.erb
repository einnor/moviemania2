<div class="row">

	<% @reservations.each do |reservation| %>
	<div class="transactionlist">
		<div class="panel">
			<div class="row">
				<div class="large-2 columns">
					<p><%%= "#{reservation.fname} #{reservation.sname}"%></p>
				</div>
				<div class="large-2 columns">
					<p><%= reservation.email %></p>
				</div>
				<div class="large-2 columns">
					<p><%= reservation.phone %></p>
				</div>
				<div class="large-2 columns">
					<p><%= reservation.cinema %></p>
				</div>
				<div class="large-2 columns">
					<p><%= reservation.nowshowing %></p>
				</div>
			</div>
			<p>
				<%= link_to "Show", "#" %>
				<%= link_to "Edit", "#" %>
				<%= link_to "Destroy", "#" %>
			</p>
		</div>
	</div>
	<% end %>

</div>
